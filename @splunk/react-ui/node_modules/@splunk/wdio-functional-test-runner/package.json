{
  "name": "@splunk/wdio-functional-test-runner",
  "version": "6.2.0",
  "license": "UNLICENSED",
  "author": "Splunk",
  "scripts": {
    "build": "babel src -d . --copy-files",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:ci": "yarn run lint -- -f junit -o test-reports/lint-results.xml",
    "start": "babel src -d . --copy-files --watch",
    "test:fixture": "wdio test/fixtures/functional.local.conf.js",
    "test:fixture:coverage": "NODE_ENV=test-coverage wdio test/fixtures/functional.local.conf.js --coverage",
    "test:fixture:cloud": "wdio test/fixtures/functional.cloud.conf.js",
    "test:remote": "wdio test/remote/functional.local.conf.js",
    "test:remote:cloud": "wdio test/remote/functional.cloud.conf.js",
    "test:remote:coverage": "NODE_ENV=test-coverage wdio test/remote/functional.local.conf.js --coverage"
  },
  "dependencies": {
    "@splunk/test-runner-utils": "^0.3.0",
    "@splunk/webpack-configs": "^1.0.1",
    "aws-sdk": "^2.207.0",
    "babel-plugin-istanbul": "^4.1.4",
    "commander": "^2.15.0",
    "execa": "^0.9.0",
    "find-up": "^2.1.0",
    "fs-extra": "^5.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "intel": "^1.2.0",
    "istanbul-lib-coverage": "^1.1.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.4",
    "mime-type": "^3.0.5",
    "tar": "^4.4.0",
    "wdio-junit-reporter": "^0.4.2",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-sauce-service": "^0.4.8",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "wdio-static-server-service": "^1.0.1",
    "webdriverio": "^4.12.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "@splunk/babel-preset": "^1.1.0",
    "@splunk/css-loader": "^1.2.0",
    "@splunk/eslint-config": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "core-js": "^2.5.3",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "phantomjs-prebuilt": "^2.1.16",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "webpack": "^3.11.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "webpack": "*"
  },
  "bin": {
    "splunk-wdio-functional-test-runner": "bin/splunk-wdio-functional-test-runner"
  },
  "repository": {
    "type": "git",
    "url": "https://git.splunk.com/scm/ui/splunk-ui.git"
  },
  "publishConfig": {
    "registry": "https://repo.splunk.com/artifactory/api/npm/npm-solutions-local/"
  },
